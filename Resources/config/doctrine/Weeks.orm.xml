<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="bet\resultBundle\Entity\Weeks" table="weeks">
    <id name="weekId" type="integer" column="week_id"/>
    <id name="leagueId" type="integer" column="league_id"/>
    <field name="dateStart" type="datetime" column="date_start" nullable="false"/>
    <field name="dateEnd" type="datetime" column="date_end" nullable="false"/>
    <field name="poissonDone" type="boolean" column="poisson_done" nullable="false"/>
  </entity>
</doctrine-mapping>
